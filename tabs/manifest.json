{
    "manifest_version": 3,
    "name": "Spotify AutoPlay",
    "description": "Automatically opens Spotify and plays a playlist",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
